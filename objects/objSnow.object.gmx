global.sky_type = 1; //Set this engines sky.

//System(s)
snow_system = part_system_create()
part_system_depth(snow_system,5)

//Particle(s)
snow = part_type_create()
part_type_alpha2(snow,1,0)
part_type_color_mix(snow,c_white,c_ltgray)
part_type_direction(snow,295,305,0,4)
part_type_speed(snow,6,7,0,0)
part_type_life(snow,300,400)
part_type_shape(snow,pt_shape_disk)
part_type_size(snow,0.05,0.1,0,0)

//Emitter(s)
emitter = part_emitter_create(snow_system)

//Result(s)
part_emitter_region(snow_system,emitter,-500,room_width+500,-30,-20,ps_shape_rectangle,ps_distr_linear); //Set the region
part_emitter_stream(snow_system,emitter,snow,8); //Continually stream snow
repeat(100) {part_system_update(snow_system);} //"Fast forward" the snow's movement so it's already falling steadily when first shown.
</string>
