    x-=5;
}
if keyboard_check(ord('D')){
    x+=5;
}

//Fire bullet
if(mouse_check_button_pressed(mb_left)){
    var bullet = instance_create(x,y,objBullet);
    bullet.direction = point_direction(x,y,mouse_x,mouse_y);
    bullet.speed = 10;
}
//Shake for jump
if(y == room_height/2){
    if (abs(device_get_tilt_x()) &gt; 1.5 || abs(device_get_tilt_y())&gt;1.5 || abs(device_get_tilt_z())&gt;1.5) {
        vspeed =-jump;
        jumping = true;
    }
}

if(!shiftUp){
    if(y &lt; room_height/2){
        gravity = 1; //change this for more "gravity" in drop
    }else if(y&gt;room_height/2+64){
        shiftUp = true;   
    }
}else{
    gravity = 0;
    vspeed = 0;
    jumping = false;
    if(y&gt;room_height/2){
        y-=5; //change this for different jump "reset" speed
    }else{
        y = room_height/2;
        shiftUp = false;
    }
    
}
